                android:layout_below="@+id/polamakan2"
                android:src="@drawable/pushup" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Push Up"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/gambar4"
                android:layout_gravity="center"
                android:textColor="@color/black"
                android:textSize="16dp"
                android:textStyle="bold" />



        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="8dp"
        >
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="170dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/borderr"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/gambar5"
                android:paddingLeft="16dp"
                android:layout_centerHorizontal="true"
                android:layout_width="160dp"
                android:layout_height="130dp"
                android:scaleType="fitXY"
                android:layout_below="@+id/polamakan2"
                android:src="@drawable/pushup" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Push Up"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/gambar5"
                android:layout_gravity="center"
                android:textColor="@color/black"
                android:textSize="16dp"
                android:textStyle="bold" />



        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="170dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/borderr"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/gambar6"
                android:paddingLeft="16dp"
                android:layout_centerHorizontal="true"
                android:layout_width="160dp"
                android:layout_height="130dp"
                android:scaleType="fitXY"
                android:layout_below="@+id/polamakan2"
                android:src="@drawable/pushup" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Push Up"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/gambar6"
                android:layout_gravity="center"
                android:textColor="@color/black"
                android:textSize="16dp"
                android:textStyle="bold" />



        </RelativeLayout>

    </LinearLayout>





</LinearLayout>