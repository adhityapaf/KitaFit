<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:focusableInTouchMode="true"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/inibiru"
        android:focusableInTouchMode="true">

        <Button
            android:id="@+id/button3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="21dp"
            android:background="@drawable/ic_back_blue"
            android:backgroundTint="@color/white"
            android:layout_marginStart="20dp"
            />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:layout_marginTop="16dp"
            android:layout_toRightOf="@+id/button3"
            android:text="Daftar Olahraga"
            android:textColor="@color/white"
            android:textSize="20dp"
            android:textStyle="normal"
            />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/uhuyy">

        <TextView
            android:id="@+id/cari"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:layout_marginTop="12dp"
            android:text="Cari Olahraga"
            android:textColor="@color/black"
            android:textSize="16dp"
            android:textStyle="normal" />

        <Button
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/cari"
            android:background="@drawable/ic_search_black_24dp" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="8dp"
        >
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="170dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/borderr"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/gambar1"
                android:paddingLeft="16dp"
                android:layout_centerHorizontal="true"
                android:layout_width="160dp"
                android:layout_height="130dp"
                android:scaleType="fitXY"
                android:layout_below="@+id/polamakan2"
                android:src="@drawable/pushup" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Push Up"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/gambar1"
                android:layout_gravity="center"
                android:textColor="@color/black"
                android:textSize="16dp"
                android:textStyle="bold" />



        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="170dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/borderr"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/gambar2"
                android:paddingLeft="16dp"
                android:layout_centerHorizontal="true"
                android:layout_width="160dp"
                android:layout_height="130dp"
                android:scaleType="fitXY"
                android:layout_below="@+id/polamakan2"
                android:src="@drawable/pushup" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Push Up"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/gambar2"
                android:layout_gravity="center"
                android:textColor="@color/black"
                android:textSize="16dp"
                android:textStyle="bold" />



        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="8dp"
        >
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="170dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/borderr"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/gambar3"
